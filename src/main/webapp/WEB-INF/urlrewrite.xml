<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>

	<rule>
		<from>^/artigo/listagem$</from>
		<to>/artigo-list.jsf</to>
	</rule>

	<rule>
		<from>^/artigo/(\d+)/edit$</from>
		<to>/artigo-form.jsf?id=$1</to>
	</rule>

	<rule>
		<from>^/artigo/new$</from>
		<to>/artigo-form.jsf</to>
	</rule>

</urlrewrite>